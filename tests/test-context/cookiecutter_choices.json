{
  "version": "2.0",
  "template": {
    "name": "cookiecutter-test-choices-with-inputs",
    "variables": [
      {
        "name": "full_name",
        "default": "Raphael Pierzina",
        "prompt": "What's your full name?",
        "type": "string"
      },
      {
        "name": "email",
        "default": "raphael@hackebrot.de",
        "prompt": "What's your email?",
        "type": "string"
      },
      {
        "name": "license",
        "default": "ISC",
        "prompt": "Please choose a license!",
        "type": "string",
        "choices": [
          "ISC",
          "MIT",
          "BSD3",
          "GNU-GPL3",
          "Apache2",
          "Mozilla2"
        ]
      }
    ]
  }
}
